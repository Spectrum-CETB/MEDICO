<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coderspot">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:requestLegacyExternalStorage="true">
        <activity
            android:name="com.example.coderspot.tabactivity"
            android:label="@string/title_activity_tabactivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.example.coderspot.register" />
        <activity android:name="com.example.coderspot.home" />
        <activity android:name="com.example.coderspot.Appointment"/>
        <activity android:name="com.example.coderspot.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>